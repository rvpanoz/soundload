{
  "name": "soundload",
  "version": "1.0.0",
  "description": "soundcloud client for downloading tracks and find related with ease.",
  "main": "./main-process/main.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development npm run start-dev",
    "start-dev": "electron . --debug",
    "pack": "electron-builder --dir",
    "test": "mocha --compilers js:babel-core/register --require ./test/test-helper.js \"test/*@(.js|.jsx)\" --watch --watch-extensions jsx"
  },
  "build": {
    "appId": "soundload"
  },
  "keywords": [
    "soundcloud",
    "electron",
    "javascript",
    "react"
  ],
  "author": "rvpanoz@gmail.com",
  "license": "ISC",
  "devDependencies": {
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^3.5.0",
    "cross-env": "3.1.4",
    "electron-builder": "^19.22.1",
    "electron-devtools-installer": "^2.2.0",
    "electron-prebuilt": "^1.4.13",
    "electron-reload": "^1.2.1",
    "jsdom": "^9.4.1",
    "mocha": "^3.0.2",
    "react-addons-test-utils": "^15.6.0"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "electron-json-storage": "^3.0.7",
    "electron-store": "^1.2.0",
    "id3js": "^1.1.3",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "moment": "^2.17.1",
    "node-soundcloud": "0.0.6",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-router-dom": "^4.2.2",
    "react-soundplayer": "^0.3.6",
    "request": "^2.81.0",
    "request-promise": "^4.2.1"
  }
}
